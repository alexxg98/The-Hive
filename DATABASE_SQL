CREATE TABLE registered_users(
    user_id INT(20) NOT NULL PRIMARY KEY AUTO_INCREMENT,
    email VARCHAR(255) NOT NULL,
    username VARCHAR(25) NOT NULL,
    password VARCHAR(255) NOT NULL,
    reputation_score INT(255),
    UNIQUE KEY (email),
    UNIQUE KEY (username)
);

CREATE TABLE groups(
    group_id INT(20) NOT NULL PRIMARY KEY AUTO_INCREMENT,
    group_name VARCHAR(25) NOT NULL,
    group_size INT(255) NOT NULL
);

CREATE TABLE projects(
    project_id INT(20) NOT NULL PRIMARY KEY AUTO_INCREMENT,
    project_name VARCHAR(25) NOT NULL
);
